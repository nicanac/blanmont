rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /members/{userId}/{allPaths=**} {
      allow read;
      allow write: if request.auth.uid == userId || request.auth.token.admin == true;
    }
    match /blog/{allPaths=**} {
      allow read;
      allow write: if request.auth.token.admin == true;
    }
  }
}
